class Card(object):
    value = 0
    rank = 0
    def __init__(self, card_vlaue, card_rank):
        self.value = card_vlaue
        self.rank = card_rank

    def compare_cards(self, camparator):
        if comparator.value > self.value:
            return false
        elif camparator.value < self.value:
            return true
        else:
            if compare_rank(camparator):
                return true
            else:
                return false

    def compare_rank(self, camparator):
        if self.ramk > camparator.rank:
            return true
        else :
            return false

class Thirteen(object):
    deck_of_cards = []

    def __init__(self):
        pass

    def Create_Cards(self):
        for i in range(1,4):
            for j in range(3,15):
                deck_of_cards.append(Card(i,j))

    def Get_Deck(self):
        return self.deck_of_cards



class main():
    my_game = Thirteen()
    my_game.Create_Cards()
    print(my_game.Get_Deck())
